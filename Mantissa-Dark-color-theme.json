  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#657B83"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.keyword",
        "constant"
      ],
      "settings": {
        "foreground": "#FF9F82"
      }
    },
    {
      "name": "Entities",
      "scope": [
        "entity.name"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FFCF69"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage"
      ],
      "settings": {
        "foreground": "#7BC1ED"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#C3E068"
      }
    },
    {
      "name": "Support",
      "scope": [
        "support", "meta.function-call.generic"
      ],
      "settings": {
        "foreground": "#D1ABED"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF6E6E"
      }
    }
  ]
